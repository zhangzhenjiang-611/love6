$(function(){
	$(".p_3_right_bottom .con ul li").hover(function(){
		var id = $(this).attr("param");
		var actionID = "#ext"+id;
		$(".p_3_right_bottom .con ul li").removeClass("extend");
		$(this).addClass("extend");
		$(".ext").hide();
		$(actionID).css({"display":"block"});
		$(this).find(".num").addClass("num2");
		$(this).find(".tits").addClass("tits2")
	},function(){
		$(".p_3_right_bottom .con ul li span.num").removeClass("num2");
		$(".p_3_right_bottom .con ul li span.tits").removeClass("tits2");
	})	 
	$(".gushi li a").hover(function(){
		$(".gushi li").removeClass("extend");
		$(".gushi li div").css({"display":"none"});
		//$(this).parent().removeClass("nor");
		$(this).parent().addClass("extend");
		
		$(this).next().css({"display":"block"})
	},function(){
		//$(this).parent().removeClass("extend");
	})
})
